# Example Environment Variables
# Copy this file to .env.development or .env.production and modify as needed

# API URL
REACT_APP_API_URL=https://ahp-django-backend.onrender.com

# Data Mode (hybrid/online/offline)
REACT_APP_DATA_MODE=hybrid

# Environment (development/production)
REACT_APP_ENV=development

# Debug Mode (true/false)
REACT_APP_DEBUG=true

# Log Level (debug/info/warn/error)
REACT_APP_LOG_LEVEL=debug

# Backend Repository
REACT_APP_BACKEND_REPO=https://github.com/aebonlee/ahp-django-backend

# GitHub Pages URL (for production)
PUBLIC_URL=https://aebonlee.github.io/ahp_app

# Local API URL (for development)
REACT_APP_LOCAL_API_URL=http://localhost:8000
